{
  "name": "whatsapp-bot (HASHIRA-BOT)",
  "description": " Whatsapp bot.",
  "keywords": [
    "hashira bot"
  ],
  "repository": "https://github.com/Guru322/GURU-BOT",
  "stack": "container",
  "env": {
    "DATABASE_URL": {
      "description": "your mongodb database url",
      "required": false,
      "value": ""
    },
    "SESSION_ID": {
      "description": "Guru_64375d41-fcb6-4be6-9178-7a3b7396a426",
      "required": true,
      "value": "Guru_64375d41-fcb6-4be6-9178-7a3b7396a426"
    },
    "MODE": {
      "description": "mode private",
      "required": true,
      "value": "private"
    },
    "PREFIX": {
      "description": "put any one symbol here except @ and + , leave it Blank if you want to use multiple prefix",
      "required": false,
      "value": "."
    },
     "HKEY": {
      "description": "Put your Heroku api key Here , Get one from here https://dashboard.heroku.com/account",
      "required": true,
      "value": "8ccf640e-969d-437a-8ec8-aed4c7f98a86"
    },
     "HAPP": {
      "description": "Put the Heroku app name, same as above entered",
      "required": true,
      "value": "madhashira"
    },
    "statusview": {
      "description": "make it true if you want bot to view status",
      "required": false,
      "value": "false"
    },
    "autoRead": {
      "description": "make it true if you want bot to read messages",
      "required": false,
      "value": "false"
    },
    "antidelete": {
      "description": "bot will forward deleted messages if you make it true",
      "required": false,
      "value": "false"
    },
     "REMOVEBG_KEY": {
      "description": "your RemoveBg api key",
      "required": false,
      "value": ""
    }
    },
    "buildpacks": [
    {
      "url": "heroku/nodejs"
    },
    {
      "url": "https://github.com/DuckyTeam/heroku-buildpack-imagemagick.git"
    },
    {
      "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest"
    },
    {
      "url": "https://github.com/clhuang/heroku-buildpack-webp-binaries.git"
    }
  ]
}

